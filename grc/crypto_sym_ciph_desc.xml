<?xml version="1.0"?>
<block>
  <name>sym_ciph_desc</name>
  <key>crypto_sym_ciph_desc</key>
  <category>crypto</category>
  <import>import crypto</import>
  <make>crypto.sym_ciph_desc($ciph_name, $padding, $keyfile, $random_iv, $start_iv)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Cipher Name</name>
    <key>ciph_name</key>
    <type>string</type>
  </param>

  <param>
    <name>random start iv</name>
    <key>random_iv</key>
    <type>enum</type>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>

  <param>
    <name>start iv</name>
    <key>start_iv</key>
    <type>raw</type>

  </param>

  <param>
    <name>Padding</name>
    <key>padding</key>
    <type>enum</type>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>

  <param>
    <name>Keyfile</name>
    <key>keyfile</key>
    <type>string</type>
  </param>


</block>
