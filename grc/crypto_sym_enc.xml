<?xml version="1.0"?>
<block>
  <name>Symmetric Encryption</name>
  <key>crypto_sym_enc</key>
  <category>crypto</category>
  <import>import crypto</import>
  <make>crypto.sym_enc(self.$cipher())</make>

  <param>
    <name>cipher description</name>
    <key>cipher</key>
    <type>string</type>
  </param>

  <sink>
    <name>pdus</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>pdus</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
