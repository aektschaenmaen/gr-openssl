<?xml version="1.0"?>
<block>
  <name>Generate random Key</name>
  <key>crypto_generate_key_rand</key>
  <category>crypto</category>
  <import>import crypto</import>
  <make>crypto.generate_key($keyfilename, $keylen)</make>

  <param>
    <name>Keyfile</name>
    <key>keyfilename</key>
    <type>string</type>
  </param>

  <param>
    <name>key length</name>
    <key>keylen</key>
    <type>int</type>
  </param>

</block>
